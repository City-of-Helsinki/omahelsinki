{
  "extends": [
    "react-app",
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "no-console": "warn",
    "space-infix-ops": "error",
    "prettier/prettier": "error",
    "no-unused-vars": "warn"
  },
  "plugins": ["prettier"]
}
