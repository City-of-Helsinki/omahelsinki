{
  "extends": [
    "plugin:prettier/recommended",
    "react-app",
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "rules": {
    "no-console": "warn",
    "object-curly-spacing": [2, "warn"],
    "space-infix-ops": "error",
    "prettier/prettier": "error"
  },
  "plugins": ["prettier"]
}
